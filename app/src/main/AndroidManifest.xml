<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.scoutingapp">


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Scoutingapp2022">
        <activity
            android:name="scoutingapp.select_board"
            android:exported="false" />
        <activity
            android:name="scoutingapp.endgame.endgame"
            android:exported="false" />
        <activity
            android:name="scoutingapp.auto_data_generator"
            android:exported="false" />
        <activity
            android:name="scoutingapp.CommentActivity"
            android:exported="false" />
        <activity
            android:name="scoutingapp.teleop_screen.teleop"
            android:exported="false" />
        <activity
            android:name="scoutingapp.autoscreen.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="scoutingapp.robotHitMiss" />
    </application>

</manifest>